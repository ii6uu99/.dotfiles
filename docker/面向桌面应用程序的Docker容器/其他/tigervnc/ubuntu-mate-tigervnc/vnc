service vnc1
{
        disable = no
        socket_type     = stream        
        wait            = no
        user            = nobody 
        group           = tty
        server          = /usr/bin/Xtigervnc
        server_args     = -geometry 1024x768 -depth 24 -inetd -once -query 127.0.0.1 -SecurityTypes None -desktop MATE
}
